{
  "name": "damax/user",
  "type": "library",
  "description": "User authentication and management.",
  "license": "MIT",
  "autoload": {
    "psr-4": { "Damax\\User\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Damax\\User\\Tests\\": "tests/" }
  },
  "require": {
    "php": ">=7.1",
    "beberlei/assert": "^2.9",
    "damax/common": "0.1.*@dev",
    "doctrine/collections": "^1.5",
    "doctrine/inflector": "^1.3",
    "pagerfanta/pagerfanta": "^2.0",
    "ramsey/uuid": "^3.7",
    "simple-bus/message-bus": "^3.0",
    "symfony/intl": "^4.0"
  },
  "require-dev": {
    "damax/api-auth-bundle": "^0.1.0",
    "doctrine/data-fixtures": "^1.3",
    "doctrine/orm": "^2.6",
    "friendsofphp/php-cs-fixer": "^2.10",
    "lcobucci/jwt": "4.*@dev",
    "matthiasnoback/symfony-config-test": "^3.1",
    "matthiasnoback/symfony-dependency-injection-test": "^2.3",
    "symfony/config": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/form": "^4.0",
    "symfony/http-kernel": "^4.0",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/routing": "^4.0",
    "symfony/security-core": "^4.0",
    "symfony/security-http": "^4.0",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "^4.0",
    "symfony/validator": "^4.0",
    "twig/twig": "^2.4"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "repositories": {
    "damax-common": {
      "type": "vcs",
      "url": "https://bitbucket.org/damaxsolutions/damax-common"
    }
  }
}
