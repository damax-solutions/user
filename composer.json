{
  "name": "damax/user",
  "type": "library",
  "description": "User authentication and management.",
  "license": [ "MIT" ],
  "autoload": {
    "php": ">=7.2",
    "psr-4": { "Damax\\User\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Damax\\User\\Tests\\": "tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "require": {
    "symfony/intl": "^4.0",
    "ramsey/uuid": "^3.7"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.0"
  },
  "config": {
    "bin-dir": "bin/"
  }
}
