{
  "name": "damax/user",
  "type": "library",
  "description": "User authentication and management.",
  "license": [ "MIT" ],
  "autoload": {
    "psr-4": { "Damax\\User\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Damax\\User\\Tests\\": "tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "require": {
    "php": ">=7.1",
    "symfony/intl": "^4.0",
    "ramsey/uuid": "^3.7",
    "simple-bus/message-bus": "^3.0",
    "pagerfanta/pagerfanta": "^1.0",
    "doctrine/collections": "^1.5",
    "beberlei/assert": "^2.9",
    "doctrine/orm": "^2.6"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.0",
    "symfony/security-core": "^4.0"
  },
  "config": {
    "bin-dir": "bin/"
  }
}
